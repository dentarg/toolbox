<h1>Elixir toolbox</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Hex updated at</th>
    </tr>
  </thead>
  <tbody>
    <%= for package <- @packages do %>
    <tr>
      <td><%= package.name %></td>
      <td><%= package.description %></td>
      <td><%= package.hex_updated_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>
